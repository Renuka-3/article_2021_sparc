theme(axis.text.x = element_text(angle = 360, hjust = 1)) +
#labs(x = "Location", y = "Abundance (CLR)", title = map_levels(tax, from='tax', to='genus',ps))
labs(x = "Location", y = "Abundance (%)", title = as.vector(tax_table(ps)[tax, "genus"]))
print(p)
}
